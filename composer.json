{
    "name": "uneca/census-app-scaffold",
    "description": "Census application scaffold",
    "keywords": [
        "uneca",
        "census",
        "scaffold"
    ],
    "homepage": "https://github.com/tech-acs/census-app-scaffold",
    "license": "MIT",
    "authors": [
        {
            "name": "UNECA",
            "email": "tamerat@un.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-intl": "*",
        "ext-zip": "*",
        "ext-redis": "*",
        "spatie/laravel-package-tools": "^1.9.2",
        "laravel/jetstream": "3.2",
        "livewire/livewire": "^2.0",
        "spatie/laravel-translatable": "^6.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Uneca\\Scaffold\\": "src",
            "Uneca\\Scaffold\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uneca\\Scaffold\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uneca\\Scaffold\\ScaffoldServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
